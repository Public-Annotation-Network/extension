{
  "name": "react-chrome-extension-boilerplate",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "eth-sig-util": "^2.5.3",
    "ethereumjs-util": "^7.0.3",
    "jquery": "3.4.1",
    "node-sass": "^4.14.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-draggable": "4.0.3",
    "react-feather": "2.0.3",
    "react-scripts": "3.1.2",
    "web3": "^1.2.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "webextensions": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "7.0.2"
  }
}
